<!-- For HTML5 Video tag info, see http://www.htmlgoodies.com/html5/client/how-to-embed-video-using-html5.html#fbid=y2nyMm_BHEv -->
				<%= tag("video", {controls: false, autoplay: false, loop: true, muted: false, autobuffer: true, class: "vee"}, open: true) %>
					<!-- Compatible with: Chrome(desktop), Firefox(desktop) -->
					<%= tag("source", src: @bucket.objects[get_path(video.attachment.url, "mp4")].url_for(:get, { :expires => 20.minutes.from_now, :secure => true, :secure => true, :force_path_style => true }).to_s ) %>
<% if false %>					
					<%= tag("source", src: @bucket.objects[get_path(video.attachment.url, "webm")].url_for(:get, { :expires => 20.minutes.from_now, :secure => true, :secure => true, :force_path_style => true }).to_s ) %>
					<%= tag("source", src: @bucket.objects[get_path(video.attachment.url, "ogv")].url_for(:get, { :expires => 20.minutes.from_now, :secure => true, :secure => true, :force_path_style => true }).to_s ) %>
<% end %>					

<!-- Modal -->
<div class="modal fade" id="myModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
	<%= render 'sessions/signup_modal' %>
</div>

<!-- Carousel -->
<div id="myCarousel" class="carousel slide">
	<!-- Indicators -->
    <ol class="carousel-indicators">
		<li data-target="#myCarousel" data-slide-to="0" class="active"></li>
        <li data-target="#myCarousel" data-slide-to="1"></li>
        <li data-target="#myCarousel" data-slide-to="2"></li>
    </ol>
    <div class="carousel-inner">
        <div class="item active">
<% if false %>		
			<img src="https://s3.amazonaws.com/barfly_thumbnails/phone/benchmark-00001.png" alt="Second slide">
<% end %>			
			<!-- For HTML5 Video tag info, see http://www.htmlgoodies.com/html5/client/how-to-embed-video-using-html5.html#fbid=y2nyMm_BHEv -->
			<%= tag("video", controls: false, autoplay: false, loop: true, muted: false, autobuffer: true, open: true) %>
				<source src="https://s3.amazonaws.com/barfly_full_videos/benchmark.mp4" type="video/mp4" /> <!-- AVC(H.264) format -->
				Your browser does not support the video tag.
			<%= tag("/video", {}, open: true) %>			

			<div class="carousel-caption">
				<h4><em>Event Type</em></h4>
				<h2><strong>Event Name</strong></h2>
				<p class="description">Event Description</p>
				<p><a class="btn btn-lg btn-primary" href="#" role="button">Learn More</a></p>
			</div>
			<div class="carousel-caption venue">
				<h2>Benchmark</h2>
				<p><em>Neighborhood</em></p>
			</div>

		</div>
        <div class="item">
<% if false %>		
			<img src="https://s3.amazonaws.com/barfly_thumbnails/phone/benchmark-00001.png" alt="Second slide">
<% end %>			

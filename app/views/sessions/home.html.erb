<div class="container">

	<div class="row">
		<!-- Venue thumbnails -->
		<div id="dynamic-content" class="col-md-9">
		
			<div id="venues" class="row">
				<!-- venue thumbnails are loaded by .loadContent('home') -->
			</div>
									
			<!-- Pagination -->
			<div id="pagination-links" class="text-center">
				<%= will_paginate @venues %>
			</div>	
			
		</div>
		
		<!-- Sort and Filter -->
		<div id="filter-container" class="col-md-3">
			<%= render partial: 'filter_sort' %>
		</div>
	</div>
	
</div><!-- container -->
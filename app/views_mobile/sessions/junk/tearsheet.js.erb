// alert("Used AJAX to get tearsheet");
$('#dynamic-content').html("<%= escape_javascript(render partial: 'tearsheet', locals: {venue: @venue, venue_events: @venue_events}) %>");
$('#filter-container').remove();
$('.footer-fixed-bottom').hide();
truncateText(function(){
	// Removes .active class to all items in carousel except the first - allows dotdotdot to complete execution first
	$( ".active.item" ).each(function( index ) {
		if(index != 0){
			$(this).removeClass('active');
		}
	});	
});